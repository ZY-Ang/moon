{
  "name": "moon-database",
  "version": "1.0.0",
  "description": "Databases for the moon backend",
  "main": "package.json",
  "scripts": {
    "deploy-all": "npm-run-all --parallel deploy-nonCheckoutReports deploy-paymentPayloadRecords deploy-siteSupportRequests deploy-userInformation deploy-userSecrets",
    "deploy-all-production": "npm-run-all --parallel deploy-nonCheckoutReports-production deploy-paymentPayloadRecords-production deploy-siteSupportRequests-production deploy-userInformation-production deploy-userSecrets-production",
    "deploy-all-production-force": "npm-run-all --parallel deploy-nonCheckoutReports-production-force deploy-paymentPayloadRecords-production-force deploy-siteSupportRequests-production-force deploy-userInformation-production-force deploy-userSecrets-production-force",
    "deploy-nonCheckoutReports": "cd nonCheckoutReports && npm run deploy",
    "deploy-paymentPayloadRecords": "cd paymentPayloadRecords && npm run deploy",
    "deploy-siteSupportRequests": "cd siteSupportRequests && npm run deploy",
    "deploy-userInformation": "cd userInformation && npm run deploy",
    "deploy-userSecrets": "cd userSecrets && npm run deploy",
    "deploy-nonCheckoutReports-production": "cd nonCheckoutReports && npm run deploy-production",
    "deploy-paymentPayloadRecords-production": "cd paymentPayloadRecords && npm run deploy-production",
    "deploy-siteSupportRequests-production": "cd siteSupportRequests && npm run deploy-production",
    "deploy-userInformation-production": "cd userInformation && npm run deploy-production",
    "deploy-userSecrets-production": "cd userSecrets && npm run deploy-production",
    "deploy-nonCheckoutReports-production-force": "cd nonCheckoutReports && npm run deploy-production-force",
    "deploy-paymentPayloadRecords-production-force": "cd paymentPayloadRecords && npm run deploy-production-force",
    "deploy-siteSupportRequests-production-force": "cd siteSupportRequests && npm run deploy-production-force",
    "deploy-userInformation-production-force": "cd userInformation && npm run deploy-production-force",
    "deploy-userSecrets-production-force": "cd userSecrets && npm run deploy-production-force"
  },
  "author": "Moon",
  "license": "UNLICENSED",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "serverless": "^1.32.0",
    "serverless-cloudformation-changesets": "0.0.2",
    "serverless-dynamodb-autoscaling": "^0.6.2"
  }
}
