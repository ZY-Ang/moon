{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "StmtS3ObjectAllow",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::moon-user-info/${paywithmoon.auth0.com/:sub}",
        "arn:aws:s3:::moon-user-info/${paywithmoon.auth0.com/:sub}/*"
      ]
    },
    {
      "Sid": "StmtmS3ListAllow",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::moon-user-info",
      "Condition": {
        "StringLike": {
          "s3:prefix": "moon-user-info/${paywithmoon.auth0.com/:sub}/*"
        }
      }
    }
  ]
}