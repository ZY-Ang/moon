{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::moon-user-info/${cognito-identity.amazonaws.com:sub}",
        "arn:aws:s3:::moon-user-info/${cognito-identity.amazonaws.com:sub}/*",
        "arn:aws:s3:::moon-user-info/${paywithmoon.auth0.com:sub}",
        "arn:aws:s3:::moon-user-info/${paywithmoon.auth0.com:sub}/*"
      ]
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::moon-user-info",
      "Condition": {
        "StringLike": {
          "s3:prefix": "moon-user-info/${paywithmoon.auth0.com:sub}/*"
        }
      }
    },
    {
      "Sid": "VisualEditor2",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::moon-user-info",
      "Condition": {
        "StringLike": {
          "s3:prefix": "moon-user-info/${cognito-identity.amazonaws.com:sub}/*"
        }
      }
    }
  ]
}
