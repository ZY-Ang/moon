    "babel-plugin-source-map-support": "^2.0.1",
    "serverless": "^1.33.2",
    "serverless-plugin-elastic-beanstalk": "^1.5.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "express": "^4.16.4",
    "gdax": "^0.8.0",
    "source-map-support": "^0.5.9"
