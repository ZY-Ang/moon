{
  "name": "moon-backend-elasticbeanstalk",
  "version": "1.0.0",
  "description": "Elastic beanstalk instances for moon backend",
  "main": "index.js",
  "scripts": {
    "start-coinbase-pro-exchange-rates": "cd CoinbaseProExchangeRates && node index.js",
